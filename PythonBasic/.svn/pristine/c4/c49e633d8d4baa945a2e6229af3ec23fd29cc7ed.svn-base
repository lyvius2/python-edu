
# str type 데이터 만들어서 id를 변수에 담기

myComment = "abcdef"

print("id:", id(myComment))
print("길이:", len(myComment))
print("e의 포함횟수:", myComment.count("e"))
print("시작하는 글자 확인", myComment.startswith("a"))

name1="김구라"
name2="개구라"
name3="김구라"

print("name1 id:", id(name1))
print("name2 id:", id(name2))
print("name3 id:", id(name3))

# 문자열의 내용을 비교연산자로 비교하기
isEqual1 = name1 == name2
isEqual2 = name1 == name3
isEqual3 = name1 != name3
print("isEqual1:", isEqual1)
print("isEqual2:", isEqual2)
print("isEqual3:", isEqual3)